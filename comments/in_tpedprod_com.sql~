COMMENT ON TABLE  in_tpedprod             IS 'Tabla encargada de almacenar los produtos de los pedidos';
COMMENT ON COLUMN in_tpedprod.pedprod_pedprod  IS 'llave primaria de la tabla ';
COMMENT ON COLUMN in_tpedprod.pedprod_pedi     IS 'llave foranea de la tabla in_tpedi ';
COMMENT ON COLUMN in_tpedprod.pedi_dska  IS 'llave foranea de la tabla in_tdska';
COMMENT ON COLUMN in_tpedprod.pedi_precio  IS 'precio del pedido';



